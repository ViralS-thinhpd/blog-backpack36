{
    "name": "thinhpd/blog",
    "description": ":description",
    "license": "license",
    "authors": [
        {
            "name": "author name",
            "email": "author email",
            "homepage": "author homepage"
        }
    ],
    "homepage": "https://github.com/thinhpd/blog",
    "keywords": ["Laravel", "Blog"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Thinhpd\\Blog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thinhpd\\Blog\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thinhpd\\Blog\\BlogServiceProvider"
            ],
            "aliases": {
                "Blog": "Thinhpd\\Blog\\Facades\\Blog"
            }
        }
    }
}
